<!DOCTYPE>
<html>
<head>
	<title>The Daily Planet - Articles</title>
  <link rel="stylesheet" href="stylesheets/main.css">
  <link href='http://fonts.googleapis.com/css?family=Ubuntu' rel='stylesheet' type='text/css'>
</head>
<body>
  <ul id="nav">
    <li><u><a href="/">Home</a></u></li>
    <li><u><a href="/articles">Articles</a></u></li>
    <li><u><a href="/articles/new">Submit</a></u></li>
    <li><u><a href="/about">About</a></u></li>
    <li><u><a href="/contact">Contact Us</a></u></li>
  </ul>
  <div class="moveit">
	<h2>Articles</h2>
	  <% articleList.forEach(function (article) { %>
    <div>
      <h2>
        <a href="/articles/<%= article.id %>"><%= article.title %></a>
      </h2>
      <div>
        <%= article.author %>
      </div>
    </div>
  <% }) %>
  </div>
</body>
</html>